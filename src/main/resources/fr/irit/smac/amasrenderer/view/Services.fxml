<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
	xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="fr.irit.smac.amasrenderer.controller.ServicesController">
	<children>
		<VBox maxHeight="-Infinity" maxWidth="1.7976931348623157E308"
			minHeight="-Infinity" prefHeight="200.0" prefWidth="200.0"
			VBox.vgrow="ALWAYS">
			<children>
				<Label alignment="CENTER" maxHeight="100.0" maxWidth="1.7976931348623157E308"
					minHeight="-Infinity" prefHeight="100.0" prefWidth="200.0"
					style="-fx-background-color: #CCCCCC;" text="INFRASTRUCTURE"
					textFill="WHITE" VBox.vgrow="ALWAYS">
					<font>
						<Font name="OpenSymbol" size="20.0" />
					</font>
				</Label>
				<StackPane prefHeight="150.0" prefWidth="200.0">
					<children>
						<Label fx:id="infrastructureLabel" maxWidth="1.7976931348623157E308"
							prefWidth="150.0">
							<StackPane.margin>
								<Insets left="10.0" right="10.0" />
							</StackPane.margin>
							<font>
								<Font name="OpenSymbol" size="15.0" />
							</font>
						</Label>
					</children>
				</StackPane>
			</children>
		</VBox>
		<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
			prefHeight="900.0" prefWidth="382.0" VBox.vgrow="ALWAYS">
			<children>
				<Label alignment="CENTER" maxHeight="-Infinity" maxWidth="1.7976931348623157E308"
					minHeight="-Infinity" prefHeight="100.0" style="-fx-background-color: #CCCCCC;"
					styleClass="title" text="SERVICES" textFill="WHITE">
					<font>
						<Font name="OpenSymbol" size="20.0" />
					</font>
				</Label>
				<Button fx:id="buttonAddService" maxHeight="-Infinity"
					maxWidth="1.7976931348623157E308" minHeight="-Infinity"
					mnemonicParsing="false" onAction="#addService" prefHeight="50.0"
					style="-fx-background-color: #FFFFFF;" text="Ajouter service"
					VBox.vgrow="ALWAYS">
					<font>
						<Font name="OpenSymbol" size="20.0" />
					</font>
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
				</Button>
				<ListView fx:id="listServices" maxHeight="1.7976931348623157E308"
					maxWidth="1.7976931348623157E308" prefHeight="753.0" prefWidth="200.0"
					style="-fx-background-color: #F8F8F8;" VBox.vgrow="ALWAYS" />
				<Button maxHeight="-Infinity" maxWidth="1.7976931348623157E308"
					minHeight="-Infinity" mnemonicParsing="false" prefHeight="70.0"
					style="-fx-background-color: #CA2723;" text="Générer" textFill="WHITE"
					VBox.vgrow="ALWAYS">
					<font>
						<Font name="OpenSymbol" size="20.0" />
					</font>
					<cursor>
						<Cursor fx:constant="HAND" />
					</cursor>
				</Button>
			</children>
		</VBox>
	</children>
</VBox>
